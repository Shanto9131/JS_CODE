function Appl(x ,y,score) {
     this.x = x;
     this.y = y;
    this.score = score;

    return this;
}
var app1 = new Appl(10,20,400);

function Apple (color,weight) {
    this.color = color;
    this.weight = weight;
   

}
Apple.prototype = {
    eat : function() {return "eat apple";},
    throw : function(){return "throw apple";}

}

var appp1 = new Apple("any",90);
var app2 = new Apple("hlw",1029);


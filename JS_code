function makecoffe ( sugar , milk ) {
    var ins = "Boil water ";
    ins+=" pout cup";
    ins+="add"+sugar+"spoons of sugar";
    ins+="add"+milk;
    
    return ins;
}
console.log(makecoffe(2 ,20));


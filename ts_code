//tuple or MAP

let users : [number,string];

users = [1,"shanto"];
users.push(2,"Rafi");
users.push(3,"Rafi");
users.push(4,"Rafi");
users.push(5,"Rafi");

console.log(users);


//enum Type --> 1.Numeric 2.string 3.heteroginious 

//1.Numeric
enum enType {
    read =1,
    save =3,
    delete,
}

console.log(enType);

//2.string

enum senType {
    read = "Shanto",
    update = "Rana",
}

console.log(senType.read);
console.log(senType.update);




